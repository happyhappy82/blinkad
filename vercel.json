{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "blog.blinkad.kr"
        }
      ],
      "destination": "/blog/index.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "blog.blinkad.kr"
        }
      ],
      "destination": "/blog/:path*"
    }
  ]
}
